#: shell/shell.cpp:980
msgid "Displays or changes the internal time.\n"
msgstr "Affiche ou modifie l’heure du système.\n"

#: shell/shell.cpp:981
msgid "The specified time is not correct.\n"
msgstr "L’heure spécifiée n’est pas correcte.\n"

#: shell/shell.cpp:982
msgid "Current time: "
msgstr "Heure actuelle: "

#: shell/shell.cpp:983
#, c-format
msgid "Type 'time %s' to change.\n"
msgstr "Tapez 'time %s' pour la modifier.\n"

#: shell/shell.cpp:988
#, c-format
msgid "Unable to make: %s.\n"
msgstr "Impossible de créer: %s.\n"

#: shell/shell.cpp:989
#, c-format
msgid "Unable to remove: %s.\n"
msgstr "Impossible de supprimer : %s.\n"

#: shell/shell.cpp:990
#, c-format
msgid "Unable to rename: %s.\n"
msgstr "Impossible de renommer : %s.\n"

#: shell/shell.cpp:991
#, c-format
msgid "Unable to get attributes: %s\n"
msgstr "Impossible d’obtenir les attributs : %s\n"

#: shell/shell.cpp:992
#, c-format
msgid "Unable to set attributes: %s\n"
msgstr "Impossible de définir les attributs : %s\n"

#: shell/shell.cpp:993
#, c-format
msgid "Unable to delete: %s.\n"
msgstr "Impossible de supprimer: %s.\n"

#: shell/shell.cpp:994
msgid ""
"All files in directory will be deleted!\n"
"Are you sure [Y/N]?"
msgstr ""
"Tous les fichiers dans le répertoire seront supprimés!\n"
"Êtes-vous sûr [O/N]?"

#: shell/shell.cpp:995
msgid "The syntax of the command is incorrect.\n"
msgstr "La syntaxe de cette commande est incorrecte.\n"

#: shell/shell.cpp:996
#, c-format
msgid "Environment variable %s not defined.\n"
msgstr "La variable d'environnement %s n'est pas définie.\n"

#: shell/shell.cpp:997
msgid "Not enough environment space left.\n"
msgstr "Pas assez d'espace d'environnement restant.\n"
